{
    "name": "graphic-portfolio",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "rm -rf ./node_modules/.vite && unlink yarn.lock && yarn update && yarn install && yarn start",
        "update": "npx ncu -u",
        "lint": "npx eslint .",
        "fix": "npx eslint --fix -c ./.eslintrc.json ./src",
        "format": "npx prettier --write ./src --config ./.prettierrc.json",
        "start": "vite --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "prepare": "husky install",
        "tsc": "npx tsc --noEmit"
    },
    "dependencies": {
        "@emailjs/browser": "^4.4.1",
        "@mongez/cache": "^1.2.4",
        "@mongez/config": "^1.0.26",
        "@mongez/dom": "^1.1.2",
        "@mongez/events": "^2.1.0",
        "@mongez/http": "^2.2.10",
        "@mongez/huskier": "^2.0.0",
        "@mongez/localization": "^2.2.4",
        "@mongez/react": "^3.1.0",
        "@mongez/react-atom": "^5.0.0",
        "@mongez/react-form": "^2.1.3",
        "@mongez/react-helmet": "^1.0.9",
        "@mongez/react-hooks": "^1.3.3",
        "@mongez/react-localization": "^1.0.5",
        "@mongez/react-router": "^2.4.1",
        "@mongez/reinforcements": "^2.3.10",
        "@mongez/supportive-is": "^2.0.3",
        "@mongez/user": "^1.0.13",
        "@tabler/icons": "^3.12.0",
        "@tabler/icons-react": "^3.12.0",
        "clsx": "^2.1.1",
        "emailjs": "^4.0.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hot-toast": "^2.4.1",
        "react-intersection-observer": "^9.13.0",
        "sass": "^1.77.8",
        "tailwind-merge": "^2.5.2",
        "taos": "^1.0.5",
        "typewriter-effect": "^2.21.0"
    },
    "devDependencies": {
        "@mongez/vite": "^2.0.4",
        "@types/crypto-js": "^4.2.2",
        "@types/node": "^22.3.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^8.1.0",
        "@typescript-eslint/parser": "^8.1.0",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.9.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.35.0",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-unused-imports": "^4.1.3",
        "npm-check-updates": "^17.0.6",
        "postcss": "^8.4.41",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.0.0",
        "tailwindcss": "^3.4.10",
        "tailwindcss-animated": "^1.1.2",
        "typescript": "^5.5.4",
        "vite": "^5.4.1",
        "vite-plugin-svgr": "^4.2.0"
    },
    "huskier": {
        "hooks": {
            "pre-commit": [
                "yarn format",
                "yarn fix --max-warnings 0",
                "yarn tsc"
            ]
        }
    }
}
